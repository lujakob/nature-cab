{
    "plugins": [
        "react-native-web/babel",
        "transform-class-properties",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ],
    "presets": [
        "stage-2",
        "react-native",
        [
            "env",
            {
                "targets": {
                    "node": "current",
                    "browsers": ["last 2 versions"]
                }
            }
        ]
    ]
}
